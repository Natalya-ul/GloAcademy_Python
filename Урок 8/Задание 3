print('Введите натуральное число:')
number_input = int(input())
number = number_input
total = 0
while number != 0:
      last_digit = number % 10
      total += last_digit
      number = number // 10
if number_input % total == 0:
      print('YES - данное число является замечательным')
else:
      print('NO - данное число не является замечательным')
