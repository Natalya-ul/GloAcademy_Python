def list_max(list):
    for i in range(len(list)):
        list[i] = int(list[i])
    return max(list)
print('Введите через пробел элементы первого списка: ')
list1 = input().split()
print('Введите через пробел элементы второго списка: ')
list2 = input().split()
total = list_max(list1) * list_max(list2)
print('Произведение двух максимальных элементов списков равно:', total)
