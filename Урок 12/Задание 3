def list_int(num_month):
            day_30 = [4, 6, 9, 11]
            day_31 = [1, 3, 5, 7, 8, 10, 12]
            day_29 = [2]
            if num_month in day_30:
                        day = 30
            if num_month in day_31:
                        day = 31
            if num_month in day_29:
                        day = 29
            return(day)

print('Введите номер месяца: ')
number = int(input())
print(f'Количество дней в месяце: {list_int(number)}')
