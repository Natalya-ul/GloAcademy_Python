print('Введите первое целое число:')
num = int(input())
print('Введите второе целое число:')
num2 = int(input())
print(num, 'умноженное на', num2, 'равно', num * num2)
print(num, 'деленное на', num2, 'равно', num / num2)
print(num, 'нацело деленное на', num2, 'равно', num // num2)
print('Остаток от деления', num, 'на', num2, 'равен', num % num2)
print(num, 'в степени', num2, 'равно', num ** num2)
