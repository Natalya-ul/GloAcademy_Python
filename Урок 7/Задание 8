print('Введите натуральное число (кол-во чисел в последовательности):')
amount_num = int(input())
print('Введите последовательность чисел: ')
number = int(input())
maximum = number
minimum = number
for i in range(1, amount_num):
    number_for = int(input())
    if number_for > maximum:
        maximum = number_for
    if number_for < minimum:
        minimum = number_for
print('Максимум равен', maximum)
print('Минимум равен', minimum)

